//integer arithmetic
{
    a: int = 5;
    b: int = 2;
    if (
        a + b == 7 and 
        b + a == 7 and 
        a - b == 3 and 
        b - a == -3 and 
        b * a == 10 and
        a * b == 10 and
        a / b == 2 and
        b / a == 0 and
        a % b == 1 and 
        b % a == 2) 
    {
        print "Variable - integer arithmetic: Passed";     
    } else {
        print "Variable - integer arithmetic: Failed";     
    }
}

